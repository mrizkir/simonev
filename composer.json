{
    "name": "mrizkir/simonev",
    "description": "SIMONEV - Sistem Informasi Monitoring dan Evaluasi Pembangunan Daerah",
    "keywords": ["regional development", "SIMONEV"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "box/spout": "^3.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "6.4.*",
        "laravel/passport": "^8.0",
        "laravel/tinker": "^1.0",
        "laravel/ui": "^1.1",
        "laravelcollective/html": "^6.0",
        "mpdf/mpdf": "^8.0",
        "phpoffice/phpspreadsheet": "^1.9",
        "spatie/laravel-activitylog": "^3.9.0",
        "spatie/laravel-permission": "3.2.*"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "cache-files-ttl":0
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
